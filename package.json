{
  "name": "grep-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "api": "pnpm pull-api && pnpm gen-api",
    "gen-api": "pnpm openapi --input src/api/spec.json --output src/api/services --client axios --useOptions --useUnionTypes --exportCore false",
    "pull-api": "curl -o src/api/spec.json http://localhost:8080/v3/api-docs"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "@primevue/forms": "^4.3.3",
    "@studiometa/vue-mapbox-gl": "^2.7.2",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.2",
    "jwt-decode": "^4.0.0",
    "mapbox-gl": "^3.11.0",
    "pinia": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.3",
    "vue": "^3.5.13",
    "vue-i18n": "10",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@primevue/auto-import-resolver": "^4.3.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "^1.1.38",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "3.5.3",
    "prettier-plugin-vue": "^1.1.6",
    "start-server-and-test": "^2.0.10",
    "typescript": "~5.8.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
